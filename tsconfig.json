{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": ["@hugeicons/core-free-icons", "@hugeicons/react-native"],
    "paths": {
      "~/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts", 
    "metro.config.js", 
    "types/uniwind-types.d.ts" 
  ]
}
